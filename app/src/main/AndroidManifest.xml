<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.baoyachi.rxjavaapidemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".app.RxJavaApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".create.CreateActivity"
            android:label="@string/title_activity_create"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity
            android:name=".create.JustActivity"
            android:label="@string/title_activity_create"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".create.FromActivity"
            android:label="@string/title_activity_create"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".create.DeferActivity"
            android:label="@string/title_activity_create"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".create.IntervalActivity"
            android:label="@string/title_activity_create"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".create.TimerActivity"
            android:label="@string/title_activity_create"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>